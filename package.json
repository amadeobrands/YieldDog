{
  "name": "yielddog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "source .env && forge test --fork-url $MAINNET_RPC_URL --fork-block-number $FORK_BLOCK_NUMBER",
    "test:watch": "source .env && forge test --fork-url $MAINNET_RPC_URL --fork-block-number $FORK_BLOCK_NUMBER -vvv --watch",
    "anvil-mainnet": "source .env && anvil --fork-url $MAINNET_RPC_URL --fork-block-number $FORK_BLOCK_NUMBER",
    "deploy:local": "source .env && forge script Deploy --broadcast --rpc-url \"$FOUNDRY\"",
    "deploy:sepolia": "source .env && forge script Deploy --broadcast --rpc-url \"$SEPOLIA_RPC_URL\" --private-key=$PRIVATE_KEY --verify --chain 11155111 --watch",
    "deploy:taiko": "source .env && forge script Deploy --rpc-url \"$TAIKO\" --private-key=$PRIVATE_KEY --broadcast --verify --verifier blockscout --verifier-url https://explorer.test.taiko.xyz/api?",
    "deploy:linea-testnet": "source .env && forge script Deploy --broadcast --rpc-url \"$LINEA_TEST_RPC_URL\" --private-key=$PRIVATE_KEY",
    "deploy:mantle-testnet": "source .env && forge script Deploy --broadcast --rpc-url \"$MANTLE_TEST_RPC\" --private-key=$PRIVATE_KEY --legacy"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@rainbow-me/rainbowkit": "^1.0.11",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "encoding": "^0.1.13",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "lokijs": "^1.5.12",
    "lucide-react": "^0.276.0",
    "next": "13.4.19",
    "pino-pretty": "^10.2.0",
    "postcss": "8.4.29",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "^2.8.0",
    "swr": "^2.2.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.2.2",
    "use-debounce": "^9.0.4",
    "viem": "^1.10.8",
    "wagmi": "^1.4.1"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false,
    "net": false,
    "tls": false
  }
}
